<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.reduxandroid">

    <application
        android:name=".ReduxAndroidApplication"
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.ReduxAndroid">
        <activity
            android:name=".potholes.PotholeActivity"
            android:exported="true"
            android:label="@string/title_activity_pothole"
            android:theme="@style/Theme.ReduxAndroid" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".calculator.CalculatorActivity"
            android:exported="true">
        </activity>
    </application>

</manifest>